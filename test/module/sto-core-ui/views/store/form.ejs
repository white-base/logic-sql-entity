<h1><%= title %></h1>
<p><%= message %></p>
<form action="<%= basePath %>/add" method="POST">
<input type="text" name="sto_id" placeholder="sto_id" required>
<input type="text" name="sto_name" placeholder="sto_name" required>
<input type="number" name="status_cd" placeholder="status_cd" required>
<% for (const key in output.columns) { %>
    <% if (output.columns[key].isDynamic && output.columns[key].visible) { %>
        <input type="text" name="<%= key %>" placeholder="<%= key %>" <% if (output.columns[key].nullable) { %> required <% } %> >
    <% } %>
<% } %>
<button type="submit">Add User</button>
</form>